---
interface Props {
    subevents: any[];
    dates: string;
}

const { subevents, dates } = Astro.props;
---

{
    (
        <ul>
            {subevents
                .filter(
                    (subevent: any) =>
                        subevent.frontmatter.dates.some((d: string) => dates.includes(d)) == true && subevent.frontmatter.published == true,
                )
                .map((subevent: any) => (
                    <li>{subevent.frontmatter.title}</li>
                ))}
        </ul>
    )
}
